{
  "languageserver": {
    "lua": {
      "command": "lua-language-server",
      "filetypes": ["lua"]
    },
    "systemd-language-server": {
          "command": "systemd-language-server",
          "filetypes": ["systemd"]
    },
    "csharp": {
      "command": "csharp-ls",
      "rootPatterns": [
        "*.sln",
        "git"
      ],
      "filetypes": ["cs", "csharp"]
    }
  },
}
