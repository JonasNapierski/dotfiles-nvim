{
  "languageserver": {
    "lua": {
      "command": "coc-lua",
      "filetypes": ["lua"]
    },
    "systemd-language-server": {
          "command": "systemd-language-server",
          "filetypes": ["systemd"]
    },
    "csharp": {
      "command": "csharp-ls",
      "rootPatterns": [
        "*.sln",
        ".csproj",
        "git"
      ],
      "filetypes": ["cs", "csharp", "xaml.cs"]
    },
    "clangd": {
      "command": "clangd",
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcc"]
    },
    "avalonia": {
      "command": "dotnet",
      "args": ["~/.var/app/com.visualstudio.code/data/vscode/extensions/avaloniateam.vscode-avalonia-0.0.31/avaloniaServer/AvaloniaLanguageServer.dll"],
      "filetypes": ["axaml"],
      "rootPatterns": [".git",".csproj"]
    }
  },
  "snippets.ultisnips.pythonPrompt": false
}
